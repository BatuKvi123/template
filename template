-- StarterGui.HubTemplate.Main.close.LocalScript
local function C_12()
local script = G2L["12"];
	local frame = script.Parent.Parent.Parent.Main
	local open = script.Parent.Parent.Parent.open
	
	
	script.Parent.MouseButton1Down:Connect(function()
		frame.Visible = false
		open.Visible = true
	end)
end;
task.spawn(C_12);
-- StarterGui.HubTemplate.open.LocalScript
local function C_14()
local script = G2L["14"];
	local frame = script.Parent.Parent.Main
	
	
	script.Parent.MouseButton1Down:Connect(function()
		frame.Visible = true
		script.Parent.Visible = false
	end)
end;
task.spawn(C_14);

