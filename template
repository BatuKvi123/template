-- StarterGui.HubTemplate.Main.createButton
local function C_7()
local script = G2L["7"];
	local frame = script.Parent.ScrollingFrame
	
		
	
	
	
	--Color Function
	local function bcolor(color)
		script.Parent.BackgroundColor3 = color
	end
	
	
	--Title Function
	local function title(titletext)
		script.Parent.title.Text = titletext
	end
	--Desc Function
	local function desc(text)
		script.Parent.info.Desc.Text = text
	end
	
	
	
	
	
	
	
	--Create Button Function
	local function createButton(name, text, callback)
		local button = Instance.new("TextButton", frame)
		local uicorner = Instance.new("UICorner", button)
		button.Name = name
		button.Text = text
		button.Size = UDim2.new(0, 50, 0, 50)
		button.BackgroundTransparency = 0.7
		button.BackgroundColor3 = Color3.new(0, 0, 0)
		button.Font = Enum.Font.SourceSans
		button.TextScaled = true
		button.Font = Enum.Font.GothamBold
		button.TextColor3 = Color3.new(1, 1, 1)
		button.MouseButton1Down:Connect(callback)
		return button
	end
	
	
	--local testing = createButton("name?", "text?", function()
	--script?
	--end)
	
	local walkspeed = createButton("walkspeedbutton", "speed", function()
		desc("gives speed to your character")
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 100
	end)
	
	
	local jumppower = createButton("jumppowerbutton", "jump", function()
		desc("gives jumppower to your character")
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = 100
	end)
	
end;
task.spawn(C_7);
-- StarterGui.HubTemplate.Main.nameofplayer.LocalScript
